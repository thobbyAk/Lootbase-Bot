{
  "name": "dispay",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create nicolas7894/dispay --node https://api.thegraph.com/deploy/",
    "create-local": "graph create nicolas7894/dispay --node http://127.0.0.1:8020",
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "deploy": "graph deploy nicolas7894/dispay --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy nicolas7894/dispay --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
    "watch-local": "graph deploy nicolas7894/dispay --watch --debug --node http://127.0.0.1:8020/ --ipfs http://localhost:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.28.1",
    "@graphprotocol/graph-ts": "^0.26.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}